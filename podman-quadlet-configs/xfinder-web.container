[Unit]
Description=X-Finder Archery Shop Frontend Web
After=network-online.target xfinder-api.service
Wants=network-online.target

[Container]
Image=docker.io/fctassinari/xfinder-web:latest
ContainerName=xfinder-web
Network=nt-xfinder.network
PublishPort=8083:8080
Volume=xfinder-web-logs:/var/log/nginx
Timezone=America/Sao_Paulo

[Service]
Restart=always
TimeoutStartSec=300

[Install]
WantedBy=multi-user.target default.target
